{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "paths": {
      "@pages/*": [
        "pages/*"
      ],
      "@pages": [
        "pages"
      ],
      "@containers/*": [
        "containers/*"
      ],
      "@containers": [
        "containers"
      ],
      "@recoil/*": [
        "recoil/*"
      ],
      "@recoil": [
        "recoil"
      ],
      "@mocks/*": [
        "mocks/*"
      ],
      "@mocks": [
        "mocks"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@layouts": [
        "layouts"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@assets": [
        "assets"
      ],
      "@components/*": [
        "components/*"
      ],
      "@components": [
        "components"
      ],
      "@apis/*": [
        "apis/*"
      ],
      "@apis": [
        "apis"
      ],
    }
  },
  "include": [
    "src"
  ]
}